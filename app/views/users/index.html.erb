<div id="wrapper">
	<h1>Administrator Page</h1>

	<%= link_to '<button>Add User</button>'.html_safe, new_user_path %>
	
	<div id="form-user">
		<table id="user-info" border='1' cellpadding="0" cellspacing="0">
			<tr>
				<th>User_ID</th>
				<th>First Name</th>
				<th>Last Name</th>
				<th>UserName</th>
				<th>Gender</th>
				<th>Password</th>
				<th>Email</th>
				<th>Phone Number</th>
				<th>Address</th>
				<th>Action</th>
			</tr>
		<% @users.each do |user| %> 
			<tr>
				<td><%= user.user_id %></td>
				<td><%= user.first_name %></td>
				<td><%= user.last_name %></td>
				<td><%= user.username %></td>
				<td><%= user.gender %></td>
				<td><%= user.password %></td>
				<td><%= user.email %></td>
				<td><%= user.phone_num %></td>
				<td><%= user.address %></td>
				<td colspan="2">
					<span>Edit</span>
					<span>|</span>
					<span>Delete</span>
				</td>
			</tr>
		<% end %>
			
		</table>
			

		
	</div>
</div><!-- end wrapper -->
